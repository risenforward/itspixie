{
  "name": "graphcool-cli-engine",
  "version": "0.0.6",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "author": "Tim Suchanek <tim@graph.cool>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.22",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "lint-staged": "^4.0.3",
    "nock": "^9.0.14",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "ts-jest": "^21.0.0",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "build": "rimraf dist && tsc -d",
    "lint": "tslint src/**/*.ts",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --trailing-comma all --write '*.ts' 'src/**/*.ts'",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "jest && yarn lint"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --single-quote --no-semi --trailing-comma all --write",
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "rootDir": "./src",
    "transform": {
      "^.+\\.(ts|tsx)$": "../../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "setupFiles": [
      "../test/init.js"
    ]
  },
  "dependencies": {
    "@heroku/linewrap": "^1.0.0",
    "ansi-escapes": "^3.0.0",
    "chalk": "^2.1.0",
    "debug": "^3.0.1",
    "find-up": "^2.1.0",
    "fs-extra": "^4.0.1",
    "klaw-sync": "^3.0.0",
    "lodash.ary": "^4.1.1",
    "lodash.defaults": "^4.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.identity": "^3.0.0",
    "lodash.keys": "^4.2.0",
    "lodash.maxby": "4.x",
    "lodash.merge": "4.x",
    "lodash.partial": "^4.2.1",
    "lodash.property": "^4.4.2",
    "lodash.result": "^4.5.2",
    "lodash.uniqby": "^4.7.0",
    "rwlockfile": "^1.4.8",
    "string": "3.x",
    "string-similarity": "^1.2.0",
    "strip-ansi": "^4.0.0",
    "supports-color": "^4.4.0"
  }
}
